{
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "64": "icon64.png",
            "128": "icon128.png"
        }
    },
    "version": "0.0.3",
    "author": "C3 Discover LLC <admin@c3discover.com>",
    "name": "NexSellPro",
    "key": "",
    "description": "NexSellPro: Chrome extension to assist Walmart sellers with profitability calculations and product analysis",
    "permissions": [
        "storage",
        "tabs",
        "webNavigation",
        "webRequest",
        "identity"
    ],
    "background": {
        "service_worker": "background.mjs",
        "type": "module"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://api.emailjs.com https://*.walmart.com https://accounts.google.com https://www.googleapis.com"
    },
    "oauth2": {
        "client_id": "469074340331-ecg0lufoje86bk2ultj35voasto7s3nc.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/spreadsheets",
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.walmart.com/ip/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "https://www.walmart.com/*",
        "https://accounts.google.com/*",
        "https://www.googleapis.com/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/*"
            ],
            "matches": [
                "https://www.walmart.com/*"
            ]
        }
    ]
}