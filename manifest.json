{
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "64": "icon64.png",
            "128": "icon128.png"
        }
    },
    "version": "0.0.3",
    "author": "C3 Discover LLC <admin@c3discover.com>",
    "name": "NexSellPro",
    "description": "NexSellPro: Chrome extension to assist Walmart sellers with profitability calculations and product analysis",
    "permissions": [
        "storage",
        "tabs",
        "webNavigation",
        "webRequest"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://api.emailjs.com https://www.walmart.com"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.walmart.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "https://www.walmart.com/*"
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "https://www.walmart.com/*"
            ],
            "resources": [
                "assets/*"
            ]
        }
    ]
}