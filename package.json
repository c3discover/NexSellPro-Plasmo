{
  "name": "nexsellpro",
  "displayName": "NexSellPro",
  "version": "1.0.3",
  "description": "NexSellPro: Chrome extension to assist Walmart sellers with profitability calculations and product analysis",
  "author": "C3 Discover LLC <admin@c3discover.com>",
  "contributors": [
    {
      "name": "Michael Latragna",
      "email": "michael@c3discover.com"
    }
  ],
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build && npm run clean-root",
    "package": "plasmo package",
    "start": "npm run dev",
    "clean": "rm -rf .plasmo/.build node_modules dist",
    "clean-root": "del /Q *.png *.woff2 *.css",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@parcel/watcher": "^2.5.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.5.0",
    "plasmo": "^0.90.3",
    "qs": "^6.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gauge-chart": "^0.5.1",
    "react-icons": "^5.4.0",
    "sharp": "^0.33.5",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@swc/core": "^1.7.39",
    "@swc/helpers": "^0.5.13",
    "@swc/types": "^0.1.12",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/chrome": "^0.0.293",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "ts-jest": "^29.2.6",
    "typescript": "^5.0.0"
  },
  "manifest": {
    "host_permissions": [
      "https://www.walmart.com/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "https://www.walmart.com/*"
        ]
      }
    ]
  },
  "packageManager": "pnpm@9.14.2",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}